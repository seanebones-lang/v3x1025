{
  "dashboard": {
    "id": null,
    "title": "Blue1 RAG System - Production Monitoring",
    "tags": ["blue1", "rag", "production", "automotive"],
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 39,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"blue1-rag-api\"}",
            "legendFormat": "API Status"
          },
          {
            "expr": "rate(http_requests_total{job=\"blue1-rag-api\"}[5m])",
            "legendFormat": "RPS"
          },
          {
            "expr": "rate(rag_queries_total[5m])",
            "legendFormat": "RAG Queries/sec"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"blue1-rag-api\"}[5m]))",
            "legendFormat": "95th Response Time"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "horizontal"
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"blue1-rag-api\"}[5m])",
            "legendFormat": "Total Requests"
          },
          {
            "expr": "rate(http_requests_total{job=\"blue1-rag-api\",status_code=~\"2..\"}[5m])",
            "legendFormat": "Success (2xx)"
          },
          {
            "expr": "rate(http_requests_total{job=\"blue1-rag-api\",status_code=~\"4..\"}[5m])",
            "legendFormat": "Client Error (4xx)"
          },
          {
            "expr": "rate(http_requests_total{job=\"blue1-rag-api\",status_code=~\"5..\"}[5m])",
            "legendFormat": "Server Error (5xx)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "spanNulls": false
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Response Time Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"blue1-rag-api\"}[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.90, rate(http_request_duration_seconds_bucket{job=\"blue1-rag-api\"}[5m]))",
            "legendFormat": "90th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"blue1-rag-api\"}[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"blue1-rag-api\"}[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            }
          }
        }
      },
      {
        "id": 4,
        "title": "RAG Query Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(rag_queries_total[5m])",
            "legendFormat": "Query Rate"
          },
          {
            "expr": "histogram_quantile(0.95, rate(rag_query_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Query Time"
          },
          {
            "expr": "rate(rag_queries_total{success=\"false\"}[5m])",
            "legendFormat": "Failed Queries"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Search Performance Breakdown",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(vector_search_duration_seconds_bucket[5m]))",
            "legendFormat": "Vector Search (95th)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(keyword_search_duration_seconds_bucket[5m]))",
            "legendFormat": "Keyword Search (95th)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(elasticsearch_query_duration_seconds_bucket[5m]))",
            "legendFormat": "Elasticsearch (95th)"
          },
          {
            "expr": "histogram_quantile(0.95, rate(embedding_duration_seconds_bucket[5m]))",
            "legendFormat": "Embedding (95th)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Cache Performance",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m]))",
            "legendFormat": "Hit Rate"
          },
          {
            "expr": "rate(cache_hits_total[5m])",
            "legendFormat": "Hits/sec"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "legendFormat": "Misses/sec"
          }
        ],
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 24},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "vertical"
        }
      },
      {
        "id": 7,
        "title": "DMS Integration Health",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(dms_request_duration_seconds_count{adapter=\"cdk\"}[5m])",
            "legendFormat": "CDK Requests/sec"
          },
          {
            "expr": "rate(dms_request_duration_seconds_count{adapter=\"reynolds\"}[5m])",
            "legendFormat": "Reynolds Requests/sec"
          },
          {
            "expr": "rate(dms_request_duration_seconds_count{success=\"false\"}[5m])",
            "legendFormat": "Failed Requests/sec"
          },
          {
            "expr": "histogram_quantile(0.95, rate(dms_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Response Time"
          }
        ],
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 24},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Document Indexing",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(documents_indexed_total[5m])",
            "legendFormat": "Docs/sec"
          },
          {
            "expr": "sum(documents_indexed_total)",
            "legendFormat": "Total Docs"
          }
        ],
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 24},
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "vertical"
        }
      },
      {
        "id": 9,
        "title": "System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "100 - (avg by(instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage %"
          },
          {
            "expr": "100 - ((node_filesystem_avail_bytes{mountpoint=\"/\"} * 100) / node_filesystem_size_bytes{mountpoint=\"/\"})",
            "legendFormat": "Disk Usage %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Dependency Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"elasticsearch\"}",
            "legendFormat": "Elasticsearch"
          },
          {
            "expr": "up{job=\"redis\"}",
            "legendFormat": "Redis"
          },
          {
            "expr": "up{job=\"pinecone-exporter\"}",
            "legendFormat": "Pinecone"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "vertical"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"text": "DOWN", "color": "red"},
                  "1": {"text": "UP", "color": "green"}
                },
                "type": "value"
              }
            ]
          }
        }
      },
      {
        "id": 11,
        "title": "Error Rate by Endpoint",
        "type": "table",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"blue1-rag-api\",status_code=~\"5..\"}[5m]) by (endpoint)",
            "format": "table",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 38},
        "options": {
          "showHeader": true,
          "sortBy": [{"desc": true, "displayName": "Error Rate"}]
        }
      },
      {
        "id": 12,
        "title": "Top Slow Queries",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, rate(rag_query_duration_seconds_bucket[5m]) by (query_type)))",
            "format": "table",
            "legendFormat": "{{query_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 38},
        "options": {
          "showHeader": true,
          "sortBy": [{"desc": true, "displayName": "95th Percentile"}]
        }
      }
    ]
  }
}