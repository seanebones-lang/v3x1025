# API Keys - LLM & Embeddings
ANTHROPIC_API_KEY=sk-ant-api03-your-key-here
VOYAGE_API_KEY=pa-your-voyage-key-here
COHERE_API_KEY=your-cohere-key-here

# Vector Database
PINECONE_API_KEY=your-pinecone-key-here
PINECONE_ENVIRONMENT=us-east-1-aws
PINECONE_INDEX_NAME=dealership-rag

# DMS Integration
CDK_API_KEY=your-cdk-api-key
CDK_API_URL=https://api.cdkglobal.com/v1
REYNOLDS_API_KEY=your-reynolds-api-key
REYNOLDS_API_URL=https://api.reyrey.com/v1
DMS_ADAPTER=mock

# Redis Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Celery Task Queue
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# Application Settings
API_SECRET_KEY=your-secret-key-for-api-auth-change-in-production
ENVIRONMENT=development
LOG_LEVEL=INFO

# Observability
LANGSMITH_API_KEY=your-langsmith-key-here
LANGSMITH_PROJECT=dealership-rag
LANGSMITH_TRACING=true
SENTRY_DSN=your-sentry-dsn-here

# Performance Tuning
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
TOP_K_RETRIEVAL=20
TOP_K_RERANK=5
MAX_TOKENS_GENERATION=1000
QUERY_TIMEOUT_SECONDS=30

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100
RATE_LIMIT_BURST=20

# Database
DATABASE_URL=sqlite:///./dealership_rag.db
